# -----------------------------------------------------
# INIT
# -----------------------------------------------------

# -----------------------------------------------------
# Exports
# -----------------------------------------------------
export EDITOR=nvim
export PATH="/usr/lib/ccache/bin/:$PATH"
export ZSH="$HOME/.oh-my-zsh"

# Initialize nvm
source /usr/share/nvm/init-nvm.sh

# Initialize asdf
. "$HOME/.asdf/asdf.sh"

# Initialize zoxide
eval "$(zoxide init zsh --cmd cd)"
